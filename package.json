{
  "name": "@ambiere/genesis",
  "version": "0.8.3",
  "description": "Generates server project templates",
  "main": "genesist.js",
  "type": "commonjs",
  "bin": {
    "genesis": "src/bin/genesis.js"
  },
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "cross-env NODE_ENV=test nyc tap \"test/**/*.test.js\" --allow-incomplete-coverage --allow-empty-coverage",
    "avae": "avae --count 1",
    "post:version": "conventional-changelog -p conventionalCommits -i CHANGELOG.md -s -r 1 && git add CHANGELOG.md"
  },
  "keywords": [
    "nodejs",
    "fastify",
    "express",
    "cli",
    "template-generator"
  ],
  "author": "ambiere <ambiere@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^6.0.1",
    "@fastify/cors": "^9.0.0",
    "@fastify/env": "^4.3.0",
    "@fastify/sensible": "^6.0.1",
    "commist": "^3.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.0.1",
    "fastify-plugin": "^5.0.1",
    "generify": "^4.2.0",
    "help-me": "^5.0.0",
    "kolorist": "^1.8.0",
    "minimist": "^1.2.8",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.0.0",
    "prompts": "^2.4.2",
    "split2": "^4.2.0",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@ambiere/avae": "^0.1.2",
    "cross-env": "^7.0.3",
    "dockerode": "^4.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.1",
    "nyc": "^17.0.0",
    "standard": "^17.1.0",
    "supertest": "^7.0.0",
    "tap": "^21.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ambiere/genesis.git"
  },
  "bugs": {
    "url": "https://github.com/ambiere/genesis/issues"
  },
  "homepage": "https://github.com/ambiere/genesis#readme"
